<p id="notice"><%= notice %></p>

<h1>Listing Dictionaries</h1>

<table  class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Iddictionary</th>
      <th>Iduser</th>
      <th>Name</th>
      <th>Idsourcelanguage</th>
      <th>Iddestinationlanguage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dictionaries.each do |dictionary| %>
      <tr>
        <td><%= dictionary.IDDictionary %></td>
        <td><%= dictionary.IDUSer %></td>
        <td><%= dictionary.Name %></td>
        <td><%= dictionary.IDSourceLanguage %></td>
        <td><%= dictionary.IDDestinationLanguage %></td>
        <td><%= link_to 'Show', dictionary %></td>
        <td><%= link_to 'Edit', edit_dictionary_path(dictionary) %></td>
        <td><%= link_to 'Destroy', dictionary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dictionary', new_dictionary_path %>
